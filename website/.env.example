# Nuk Library Website - Environment Variables
# Copy this file to .env and update with your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Google Services
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
REACT_APP_GOOGLE_ANALYTICS_ID=your_google_analytics_id_here

# Social Media (for API integrations if needed)
REACT_APP_INSTAGRAM_ACCESS_TOKEN=your_instagram_token_here
REACT_APP_FACEBOOK_PAGE_ID=your_facebook_page_id_here

# Optional: Feature Flags
REACT_APP_ENABLE_CHATBOT=true
REACT_APP_ENABLE_SOCIAL_FEED=true
REACT_APP_ENABLE_GOOGLE_REVIEWS=true

# Optional: Contact Email (for display)
REACT_APP_CONTACT_EMAIL=info@mynuk.com
REACT_APP_PHONE=+91_12345_67890
